+    private void updateShopCards(int i) {
+        int size = 0;
+        if (i == -1) {
+            size = Card.getCards().size() - 72;
+            i = 72;
+        } else size = 12;
+        ArrayList<ImageView> cardList = new ArrayList<>();
+        cardList.add(card11);
+        cardList.add(card12);
+        cardList.add(card13);
+        cardList.add(card14);
+        cardList.add(card21);
+        cardList.add(card22);
+        cardList.add(card23);
+        cardList.add(card24);
+        cardList.add(card31);
+        cardList.add(card32);
+        cardList.add(card33);
+        cardList.add(card34);
+
+        for (int j = 0; j < 12; j++) {
+            cardList.get(j).setImage(null);
+        }
+        for (int j = 0; j < size; j++) {
+            cardList.get(j).setImage(Util.getImage(Card.getCards().get(i++).getCardName()));
+        }
+
+    }
+
