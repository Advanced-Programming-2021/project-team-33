+            try {
+                new ShopMenu().start();
+            } catch (IOException e) {
+                e.printStackTrace();
+            }
+        });
+        leftButton.setOnMouseClicked(event -> {
+            MainMenu.playSound(Util.CLICK_MUSIC);
+            if (ref.i > 11) {
+                ref.page--;
+                ref.i -= 12;
+                updateShopCards(ref.i);
+            } else updateShopCards(0);
+        });
+        rightButton.setOnMouseClicked(event -> {
+            MainMenu.playSound(Util.CLICK_MUSIC);
+            if (ref.i < 60) {
+                ref.i += 12;
+                ref.page++;
+                updateShopCards(ref.i);
