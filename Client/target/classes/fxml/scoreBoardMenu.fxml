<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.ScoreboardMenu"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fx:id="backGround">
    </ImageView>
    <children>
        <Label text="Scoreboard" layoutX="600" layoutY="20" style="-fx-background-color: #0e466c;-fx-text-fill: white;-fx-font-size: 24"/>
        <ListView layoutX="105.0" layoutY="75.0" prefHeight="600" prefWidth="1100" fx:id="scoreBoard">

        </ListView>
    </children>
    <HBox layoutX="2" layoutY="680" >
        <Button text="Back" style="-fx-background-color: red;-fx-text-fill: white;-fx-font-size: 20" onMouseClicked="#back"/>
    </HBox>
</AnchorPane>
